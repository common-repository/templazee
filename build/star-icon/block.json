{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "templazee/star-icon",
  "version": "0.1.0",
  "title": "Star Icon",
  "icon": "info",
  "category": "templazee",
  "description": "Block to add star icon to the content.",
  "attributes": {
    "textAlign": {
      "type": "string",
      "default": "left"
    },
    "numStar": {
      "type": "number",
      "default": 3
    }
  },
  "supports": {
    "html": false,
    "color": {
      "text": true,
      "background": false
    },
    "align": true,
    "typography": {
      "fontSize": true
    }
  },
  "textdomain": "star-icon",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
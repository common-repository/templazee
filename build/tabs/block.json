{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "templazee/tabs",
  "title": "Tab",
  "icon": "screenoptions",
  "category": "widgets",
  "parent": [
    "templazee/tab"
  ],
  "description": "Single Tab block to be used with Tabs block",
  "supports": {
    "anchor": true,
    "reusable": false,
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "spacing": {
      "padding": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    },
    "__experimentalLayout": true
  },
  "textdomain": "Tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}